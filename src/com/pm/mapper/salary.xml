<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pm.dao.SalaryDao"><!-- 后面接的是接口名称 -->
	<select id="getSalaryById" parameterType="java.lang.String" resultType="java.util.Map">
	    SELECT 	T.S_ID,
       			T.E_ID,
       			T.S_BASIC,
		       	T.S_FOOD,
		       	T.S_TRAFFIC,
		       	T.S_HOTEL,
		       	T.S_OUT,
		       	T.S_CHECK,
		       	TO_CHAR(T.S_DATE,'YYYY-MM') S_DATE,
		       	T.S_TIME
		FROM T_SALARY T
	    WHERE T.E_ID=#{e_id} 
 	</select>
</mapper>





































